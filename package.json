	},
	"dependencies": {
		"uuid": "3.2.1"
	},
	"devDependencies": {
		"@types/jest": "22.1.0",
		"jest": "22.1.4",
		"nodemon": "1.14.11",
		"ts-jest": "22.0.1",
		"ts-node": "4.0.1",
		"typescript": "2.6.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok"
	],
	"repository": {
