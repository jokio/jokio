{
	"name": "jokio",
	"version": "0.3.0",
	"description": "core, awesome, simple functions",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"scripts": {
		"start": "ts-node src",
		"test": "ts-node src/_test",
		"dev": "nodemon",
		"build": "tsc",
		"deploy": "tsc && npm publish"
	},
	"dependencies": {
		"fs-extra": "^7.0.1",
		"uuid": "3.2.1"
	},
	"devDependencies": {
		"@types/jest": "23.3.10",
		"@types/node": "10.12.18",
		"jest": "23.6.0",
		"nodemon": "1.18.9",
		"ts-jest": "23.10.5",
		"ts-node": "7.0.1",
		"typescript": "3.2.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jokio/jokio.git"
	},
	"author": "Jok Entertainers",
	"license": "MIT",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
